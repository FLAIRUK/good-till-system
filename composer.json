{
    "name": "flairuk/good-till-system",
    "description": "Good Till System API for Laravel",
    "keywords": [
        "flair",
        "laravel",
        "good-till-system"
    ],
    "homepage": "https://github.com/flair/good-till-system",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Phil Graham",
            "email": "ijeffrouk@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "laravel/framework": "^6.0|^7.1"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "FLAIR\\GoodTillSystem\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FLAIR\\GoodTillSystem\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "FLAIR\\GoodTillSystem\\GoodTillSystemServiceProvider"
            ],
            "aliases": {
                "GoodTillSystem": "FLAIR\\GoodTillSystem\\GoodTillSystemFacade"
            }
        }
    }
}
